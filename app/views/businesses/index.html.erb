<div class="row">
  <div class="col-sm-7">
    <ul>
      <% @candidates.each do |candidate| %>
        <li>
          <%= candidate['name']%>
        </li>
      <% end %>
    </ul>
  </div>

  <div class="col-sm-5">
    <div
      id="map"
      style="width: 100%;
      height: 600px;"
      data-markers="<%= @markers.to_json %>"
    >
    </div>
  </div>
</div>

