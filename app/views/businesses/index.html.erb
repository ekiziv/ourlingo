<div class="row">
  <div class="col-sm-7">
    <ul>
      <% @businesses.each do |business| %>
        <li>
          <%= link_to business.name, business_path(business) %>
        </li>
      <% end %>
    </ul>
  </div>

  <div class="col-sm-5">
    <div
      id="map"
      style="width: 100%;
      height: 600px;"
      data-markers="<%= @markers.to_json %>"
    >
    </div>
  </div>
</div>

