<div class="row">
  <div class="col-sm-7">
    <% @businesses.each do |business| %>
      <%= business.name %>
      <%= business.address %>
    <% end %>
  </div>

  <div class="col-sm-5">
    <div
      id="map"
      style="width: 100%;
      height: 600px;"
      data-markers="<%= @markers.to_json %>"
    >
    </div>
  </div>
</div>




